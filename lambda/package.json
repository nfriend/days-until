{
  "name": "handle-days-until",
  "version": "2.0.0",
  "description": "Lambda function that powers the Days Until Alexa skill",
  "main": "build/index.js",
  "repository": "gitlab:nfriend/days-until",
  "author": {
    "name": "Nathan Friend",
    "email": "hello@nathanfriend.io",
    "url": "https://nathanfriend.io"
  },
  "license": "Apache License 2.0 with Common Clause Restriction",
  "private": true,
  "scripts": {
    "build": "tsc",
    "translate": "i18next-scanner --config i18next-scanner.config.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "model": "yarn generate-model && yarn merge-model && yarn copy-plain-text-content && yarn copy-english-files",
    "generate-model": "cd ../skill-package/interactionModels/custom && java -jar alexa-generate.jar ./en-US.grammar ./en-US.json && cd -",
    "merge-model": "node utility-scripts/merge-model.js",
    "copy-plain-text-content": "node utility-scripts/copy-plain-text-content.js",
    "copy-english-files": "node utility-scripts/copy-english-files.js",
    "prepush": "yarn build && yarn lint && yarn test && yarn translate && yarn model"
  },
  "dependencies": {
    "@nfriend/amazon.date-normalizer": "^1.2.3",
    "@sentry/node": "5.27.6",
    "@sentry/tracing": "5.27.6",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.10",
    "ask-sdk-core": "2.10.1",
    "ask-sdk-dynamodb-persistence-adapter": "2.10.0",
    "ask-sdk-model": "1.34.1",
    "aws-sdk": "2.799.0",
    "capitalize": "2.0.3",
    "i18next": "19.8.4",
    "lodash": "4.17.20",
    "module-alias": "^2.2.2",
    "moment": "2.29.2",
    "moment-timezone": "0.5.32",
    "natural": "2.1.5"
  },
  "devDependencies": {
    "@sentry/cli": "1.59.0",
    "@tsconfig/node12": "1.0.7",
    "@types/capitalize": "2.0.0",
    "@types/natural": "2.1.1",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "glob": "^7.1.6",
    "i18next-scanner": "3.0.0",
    "i18next-scanner-typescript": "1.0.6",
    "jest": "26.6.3",
    "jest-diff": "26.6.2",
    "jest-junit-reporter": "1.1.0",
    "lambda-local": "1.7.4",
    "mockdate": "^3.0.2",
    "prettier": "2.2.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  },
  "_moduleAliases": {
    "~": "build"
  }
}
